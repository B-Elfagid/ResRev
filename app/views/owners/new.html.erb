<h1>Owner Sign up</h1>

<%= form_for (@owner) do |f| %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <br>
    <%= f.label :Email %>
    <%= f.email_field :email %>
    <br>
    <%= f.label :Password %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Sign Up" %>

<% end %>

