

<%= form_for([@restaurant, @review]) do |f| %>
    
    <%= f.label :rating %>
    <%= f.text_field :rating %>
    <br>

    <%= f.label :date %>
    <%= f.text_field :date, :placeholder => 'mm/dd/yyyy' %>
    <br>

    <%= f.label :comment %>
    <%= f.text_field :comment %>
    <br>
 
    <%= f.label :image %>
    <%= f.file_field :image %>
   
    <%= f.submit "Submit" %>

    <% end %>